{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "redirects": [
        { 
          "source": "/oppo-catalogue",
          "destination": "/products", 
          "permanent": true
        },
        { 
          "source": "/oppo-catalogue/",
          "destination": "/products", 
          "permanent": true
        }
    ]
}